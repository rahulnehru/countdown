
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /standups              controllers.StandupController.allStandups
GET     /standups/:name        controllers.StandupController.standup(name)

# TO do change to Post
GET    /standups/:name/connect controllers.StandupController.connect(name)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file          controllers.Assets.versioned(path="/public", file: Asset)
